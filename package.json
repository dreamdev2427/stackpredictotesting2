{
  "name": "WahoopRedit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "cypress": "cypress open",
    "generate:types": "contentful-typescript-codegen --output src/@types/contentful.d.ts",
    "dev": "npm run generate:types && next dev -- -p 3005",
    "build": "npm run generate:types && next build",
    "start": "npm run generate:types && next start"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-react-renderer": "^15.17.0",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@material-tailwind/react": "^2.1.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "accept-language": "^3.0.18",
    "card-validator": "^8.1.1",
    "flowbite": "^1.6.6",
    "flowbite-react": "^0.4.9",
    "i18next": "^22.4.15",
    "i18next-resources-to-backend": "^1.1.3",
    "negotiator": "^0.6.3",
    "next": "^13.3.1",
    "next-auth": "^4.22.1",
    "next-intl": "^2.14.0-beta.4",
    "react": "18.2.0",
    "react-datepicker": "^4.14.1",
    "react-dom": "18.2.0",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.9.0",
    "react-player": "^2.12.0",
    "react-redux": "^8.0.7",
    "ts-node": "^10.9.1",
    "tw-elements": "^1.0.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.1",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-html-parser": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "autoprefixer": "^10.4.14",
    "contentful": "^10.1.8",
    "contentful-management": "^10.35.2",
    "contentful-typescript-codegen": "^3.4.0",
    "cypress": "^12.11.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
